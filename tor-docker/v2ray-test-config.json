{
    "dns": {
        "fallbackStrategy": "disabled_if_any_match",
        "servers": [
            {
                "address": "localhost",
                "domains": [
                    "full:node2.popilium.ir"
                ],
                "fallbackStrategy": "disabled",
                "queryStrategy": ""
            }
        ],
        "tag": "dns"
    },
    "inbounds": [],
    "log": {
        "loglevel": "warning"
    },
    "outbounds": [
        {
            "mux": {
                "concurrency": 8,
                "enabled": true
            },
            "protocol": "vless",
            "settings": {
                "vnext": [
                    {
                        "address": "node2.popilium.ir",
                        "port": 9553,
                        "users": [
                            {
                                "encryption": "none",
                                "id": "4bdf8807-3951-4d87-bb07-e2b84c8f45cf"
                            }
                        ]
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "utls",
                "utlsSettings": {
                    "imitate": "firefox_auto",
                    "tlsConfig": {
                        "serverName": "node2.popilium.ir"
                    }
                }
            },
            "tag": "proxy"
        },
        {
            "protocol": "freedom",
            "tag": "direct"
        },
        {
            "protocol": "freedom",
            "tag": "bypass"
        },
        {
            "protocol": "blackhole",
            "tag": "block"
        }
    ],
    "policy": {
        "levels": {
            "1": {
                "connIdle": 30
            }
        },
        "system": {
            "statsOutboundDownlink": true,
            "statsOutboundUplink": true
        }
    },
    "routing": {
        "domainMatcher": "mph",
        "domainStrategy": "AsIs",
        "rules": []
    },
    "stats": {}
}